
include ./footer-modals.pug
footer.ngl-footer
	.container
		.bottom-menu
			a.logo(href='#')
				img(src='img/logo.png' alt='')
			ul(class='bottom-menu__nav')
				li
					a(href='#') Главная
				li
					a(href='#') Мои работы
				li
					a(href='#') Вакансии
				li
					a(href='#') Друзья и партнеры
				li
					a(href='#') Обо мне
			.ngl-social
				a(href='#' class='ngl-social__link')
					svg(width='30' height='30')
						use(class="default" xlink:href='css/sprite.svg#fb')
						use(class="hover" xlink:href='css/sprite.svg#fb-hover')
				a(href='#' class='ngl-social__link')
					svg(width='30' height='30')
						use(class="default" xlink:href='css/sprite.svg#vk')
						use(class="hover" xlink:href='css/sprite.svg#vk-hover')
				a(href='#' class='ngl-social__link')
					svg(width='30' height='30')
						use(class="default" xlink:href='css/sprite.svg#ytb')
						use(class="hover" xlink:href='css/sprite.svg#ytb-hover')
				a(href='#' class='ngl-social__link')
					svg(width='30' height='30')
						use(class="default" xlink:href='css/sprite.svg#telegram')
						use(class="hover" xlink:href='css/sprite.svg#telegram-hover')
		.political
			a(href='#') Политика конфиденциальности
			a(href='#') О компании


